                    </tr>
                    <tr>
                        <td><a span><!--文章標題--></a span></td>
                        <td width="300"><font size="5"><strong><a span><%= data[0].writingsName %></font></strong></a span></td>                        
                    </tr>
					
					<tr>
                        <td><a span><!--編輯--></a span></td>
                        <td><a span><%= data[0].notes %></a span></td>                        
                    </tr> 
					
                    <tr>
                        <td><a span><!--內容--></a span></td>
                        <td style= "word-break:break-all; " width="100px"><font size="4"><PRE><a span><%= data[0].writingsContent %></a span></PRE></font></td>                        
                    </tr>
                 
                    <tr>
                        <td><a span><!--更改時間--></a span></td>
                        <td><a span><%= moment(data[0].editDate).format("YYYY-MM-DD")  %></a span></td>                        
                    </tr>                                                                              
                    <!--~~~~~~~~~~~~~~~~~~~~~~-->                     
                </table> 
